{
    "classId": "idm.componet.controlCenter.icontrolsettingpanel",
    "comName": "属性面板",
    "className": "IControlSettingPanel",
    "comType": "common",
    "comLangue": "vue",
    "compositeAttr": [
        {
            "type": "input",
            "layoutType": "inline",
            "text": "唯一标识",
            "bindKey": "ctrlId",
            "disabled": true,
            "default": "@[packageid]",
            "desc": "",
            "helpUrl": ""
        },
        {
            "type": "group",
            "text": "基本属性",
            "desc": "提供设置组件能达到展示效果的基本属性设置信息",
            "children": [
                {
                    "type": "radio",
                    "layoutType": "block",
                    "text": "控件表单布局方式",
                    "bindKey": "ctrlformLayout",
                    "dictionary": [
                        {
                            "label": "horizontal",
                            "value": "horizontal"
                        },
                        {
                            "label": "vertical",
                            "value": "vertical"
                        },
                        {
                            "label": "inline",
                            "value": "inline"
                        }
                    ],
                    "default":"horizontal"
                },
                {
                    "type": "radio",
                    "layoutType": "block",
                    "text": "按钮位置",
                    "bindKey": "ctrlButtonPosition",
                    "dictionary": [
                        {
                            "label": "表单末尾",
                            "value": "formlast"
                        },
                        {
                            "label": "底部固定",
                            "value": "fixedBottom"
                        },
                        {
                            "label": "顶部固定",
                            "value": "fixedTop"
                        }
                    ],
                    "default":"formlast"
                },
                {
                    "type": "radio",
                    "layoutType": "block",
                    "text": "按钮对齐",
                    "bindKey": "ctrlButtonAlign",
                    "dictionary": [
                        {
                            "label": "居左",
                            "value": "left"
                        },
                        {
                            "label": "居中",
                            "value": "center"
                        },
                        {
                            "label": "居右",
                            "value": "right"
                        }
                    ],
                    "default":"left"
                },
                {
                    "type": "textarea",
                    "layoutType": "block",
                    "text": "控件属性集合接口地址",
                    "bindKey": "ctrlDataIntelface",
                    "default": "/ctrl/idm/console/fetchMarketModuleAttrData"
                },
                {
                    "type": "textarea",
                    "layoutType": "block",
                    "text": "数据代码值接口地址",
                    "bindKey": "codeDataIntelface",
                    "default": "/ctrl/code/select/data"
                },
                {
                    "type": "textarea",
                    "layoutType": "block",
                    "text": "sql执行接口地址",
                    "bindKey": "sqlDataIntelface",
                    "default": "/ctrl/code/select/metaSql"
                },
                {
                    "type": "textarea",
                    "layoutType": "block",
                    "text": "sql执行接口地址",
                    "bindKey": "fileUploadDataIntelface",
                    "default": "/ctrl/idm/api/fileupload"
                },
                {
                    "type": "textarea",
                    "layoutType": "block",
                    "text": "加载文案",
                    "bindKey": "spinTip",
                    "desc":"为空则默认为“努力加载中...”",
                    "default": "努力加载中..."
                }
            ]
        },
        {
            "type": "table",
            "text": "操作按钮",
            "bindKey": "buttonList",
            "tabText": "按钮[index]",
            "children": [
                {
                    "type": "input",
                    "layoutType": "inline",
                    "text": "按钮名称",
                    "bindKey": "buttonName",
                    "default": ""
                },{
                    "type": "radio",
                    "layoutType": "inline",
                    "text": "按钮风格",
                    "bindKey": "buttonType",
                    "dictionary": [
                        {
                            "label": "主要",
                            "value": "primary"
                        },
                        {
                            "label": "次要",
                            "value": "default"
                        },
                        {
                            "label": "虚线",
                            "value": "dashed"
                        },
                        {
                            "label": "危险",
                            "value": "danger"
                        },
                        {
                            "label": "链接",
                            "value": "link"
